{
  "name": "io.pinf.ui",
  "version": "0.0.0",
  "private": true,
  "pm": "npm",
  "dependencies": {
    "fs-extra": "~0.8.1",
    "pio": "^0.6",
    "smi.cli": "0.x"
  },
  "scripts": {
    "install": "./node_modules/.bin/smi install"
  },
  "upstream": {
    "packages": {
      "top": [
        "../../*",
        "../../../../*"
      ]
    }
  },
  "mappings": {
    "io.pinf.server.www": "top/io.pinf.server.www"
  },
  "config": {
    "smi.cli": {
      "packagesDirectory": "node_modules"
    },
    "pio.deploy.converter": {
      "name": "nodejs-server"
    }
  },
  "config.plugin": {
    "io.pinf.server.firewidgets": {
    	"widgets": {
        "1-layout": "./widgets/1-layout/*",
        "2-pio": "./widgets/2-pio/*"
    	}
    }
  }
}